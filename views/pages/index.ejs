<html>
<head>
		<META http-equiv="refresh" content="2016/">
		<meta http-equiv="cache-control" content="max-age=0" />
		<meta http-equiv="cache-control" content="no-cache" />
		<meta http-equiv="expires" content="0" />
		<meta http-equiv="expires" content="Tue, 01 Jan 1980 1:00:00 GMT" />
		<meta http-equiv="pragma" content="no-cache" />
</head>
<body>
	Listen on to <%= results.year %> on <a href="https://open.spotify.com/playlist/<%=results.playlist_id %>"><img src="/images/spotify.png" /></a>
<ol>
    <% results.tracks.forEach(function(r) { %>
		<li><%= r.artist %> - "<%= r.title %>" - <%= r.album %>
			<% if (r.images && r.images.length > 0) { %> 
				<div><img src="<%= r.images.find(i => i.width < 300).url %>" /></div>
			<% } %>
			<div><%= r.comments %></div>
		</li>
	<% }); %>
</ol>

<a href="/years">All Years</a>

<body>
</html>